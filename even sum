#include <stdio.h>
int main(){
    int a[10] ;
    int sum=0,i;
    printf("enter the element of matrix:");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
        }
    for(i=0;i<10;i++)
    {
        if(a[i]%2==0)
        {
            sum=sum+a[i];
        }
       
    }
printf("sum of even elements of array is %d",sum);
}
    
